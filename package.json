{
  "name": "hackerspacesg-v2-11ty",
  "version": "0.0.2",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "watch:sass": "sass  --no-source-map --watch content/css:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass  --no-source-map content/css:_site/css",
    "build:eleventy": "eleventy",
    "postbuild": "postcss _site/css/*.css -u autoprefixer cssnano -r --no-map",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.7",
    "ical": "~0.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "sass": "^1.37.5"
  }
}
